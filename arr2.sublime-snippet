int row,col;
cin >>row >> col;
vector<vector<int>> arr2;
arr2.resize(row,vector<int>(col,0));
for(int q=0;q<row;q++){
  for(int r=0;r<col;r++){
    cin  >> arr2[q][r];
  }
}
